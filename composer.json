{
    "name": "webjump/module-webjump-path-hint",
    "license": "Copyright",
    "type": "magento2-module",
    "description": "Magento 2 Module ABIB2B Convergence",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        },
        {
            "type": "composer",
            "url": "http://www.webjumpdev.com/"
        }
    ],
    "require": {
        "php": "~5.6.0|~7.0.0",
        "magento/framework": "100.1.*"
